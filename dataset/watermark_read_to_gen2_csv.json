{
	"name": "watermark_read_to_gen2_csv",
	"properties": {
		"linkedServiceName": {
			"referenceName": "gen2_ls_kvc",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"tableName": {
				"type": "string"
			},
			"tableSchema": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat(dataset().tableSchema,'-',dataset().tableName,'-',utcNow(),'.csv')",
					"type": "Expression"
				},
				"folderPath": "watermark/tableLevel",
				"fileSystem": "kvc"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}